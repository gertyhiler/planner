{
  "name": "@planner/ui-kit",
  "version": "1.0.4",
  "type": "module",
  "description": "",
  "scripts": {
    "format": "prettier --write --parser typescript '**/*.{ts,tsx}'",
    "lint": "eslint . --ext .ts,.tsx --ignore-path .gitignore --fix",
    "build": "tsc && vite build",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "story": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky",
    "release": "publish -r --no-git-checks",
    "version": "changeset version",
    "version:change": "changeset add",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "exports": {
    "./components/*": {
      "types": "./dist/types/components/*/index.d.ts",
      "import": "./dist/esm/components/*/index.mjs",
      "require": "./dist/cjs/components/*/index.cjs"
    },
    "./tailwind": {
      "types": "./dist/types/tailwind.d.ts",
      "import": "./dist/esm/tailwind.mjs",
      "require": "./dist/cjs/tailwind.cjs"
    },
    "./utils": {
      "types": "./dist/types/lib/utils.d.ts",
      "import": "./dist/esm/lib/utils.mjs",
      "require": "./dist/cjs/lib/utils.cjs"
    },
    "./hooks": {
      "types": "./dist/types/hooks/index.d.ts",
      "import": "./dist/esm/hooks/index.mjs",
      "require": "./dist/cjs/hooks/index.cjs"
    },
    "./style.css": "./dist/style.css",
    "./tailwind.config.cjs": "./dist/preset.cjs",
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/src/index.cjs",
  "module": "dist/esm/src/index.mjs",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@changesets/cli": "^2.29.5",
    "@storybook/addon-onboarding": "^9.1.1",
    "@storybook/react-vite": "^9.1.1",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^22.10.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "autoprefixer": "^10.4.20",
    "clean-package": "^2.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-sort-exports": "^0.9.1",
    "eslint-plugin-storybook": "^9.1.1",
    "fast-glob": "^3.3.2",
    "globals": "^15.12.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.49",
    "postcss-import": "^16.1.0",
    "prettier": "^3.4.1",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "prop-types": "^15.8.1",
    "react": "^19.1.1",
    "rollup-preserve-directives": "^1.1.3",
    "storybook": "^9.1.1",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.9.2",
    "vite": "^6.0.1",
    "vite-plugin-dts": "^4.3.0",
    "vite-plugin-static-copy": "^2.2.0"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.539.0",
    "react-resizable-panels": "^3.0.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "peerDependencies": {
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwindcss": "^3.4.6"
  },
  "clean-package": {
    "indent": 2,
    "remove": [
      "devDependencies",
      "jest",
      "lint-staged",
      "clean-package"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run format",
      "npm run lint"
    ]
  }
}
